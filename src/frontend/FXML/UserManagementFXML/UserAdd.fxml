<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="215.0" prefWidth="437.0"
            xmlns="http://javafx.com/javafx/8.0.161"
            fx:controller="frontend.Controllers.UserManagementControllers.UserAddController">
    <Text layoutX="28.0" layoutY="48.0" text="User name"/>
    <Text layoutX="28.0" layoutY="92.0" text="Password"/>
    <TextField fx:id="userNameField" layoutX="106.0" layoutY="28.0" prefHeight="30.0" prefWidth="300.0"/>
    <PasswordField fx:id="passwordField" layoutX="106.0" layoutY="72.0" prefHeight="30.0" prefWidth="300.0"/>
    <Button fx:id="addButton" layoutX="275.0" layoutY="162.0" mnemonicParsing="false" onAction="#addUser"
            prefHeight="30.0" prefWidth="130.0" text="Add user"/>
    <Button fx:id="closeButton" layoutX="28.0" layoutY="162.0" mnemonicParsing="false" onAction="#Close"
            prefHeight="30.0" prefWidth="130.0" text="Back"/>
    <Label fx:id="errorLabel" layoutX="28.0" layoutY="119.0" prefHeight="30.0" prefWidth="378.0" textFill="#ff0000"/>
</AnchorPane>
