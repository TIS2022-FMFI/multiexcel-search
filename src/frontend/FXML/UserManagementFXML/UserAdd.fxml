<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="300.0" prefWidth="900.0"
            xmlns="http://javafx.com/javafx/8.0.161"
            fx:controller="frontend.Controllers.UserManagementControllers.UserAddController">
    <Text layoutX="140.0" layoutY="114.0" text="User name"/>
    <Text layoutX="140.0" layoutY="172.0" text="Password"/>
    <TextField fx:id="userNameField" layoutX="256.0" layoutY="94.0" prefHeight="30.0" prefWidth="300.0"/>
    <PasswordField fx:id="passwordField" layoutX="256.0" layoutY="155.0" prefHeight="30.0" prefWidth="300.0"/>
    <Button fx:id="addButton" layoutX="681.0" layoutY="38.0" mnemonicParsing="false" onAction="#addUser"
            prefHeight="80.0" prefWidth="150.0" text="Add user"/>
    <Button fx:id="closeButton" layoutX="681.0" layoutY="167.0" mnemonicParsing="false" onAction="#Close"
            prefHeight="80.0" prefWidth="150.0" text="Back"/>
    <Label fx:id="errorLabel" layoutX="140.0" layoutY="207.0" prefHeight="30.0" prefWidth="500.0" textFill="#ff0000"/>
</AnchorPane>
