<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.CheckBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.161"
            fx:controller="frontend.Controllers.SearchControllers.SecondSearchControllers.SecondSearchController">

    <CheckBox fx:id="check_part_number" text="Part number" layoutX="150" layoutY="20" onAction="#setVisiblePartNumber"/>
    <CheckBox fx:id="check_customer" text="Customer" layoutX="300" layoutY="20" onAction="#setVisibleCustomer"/>
    <CheckBox fx:id="check_part_name" text="Part name" layoutX="450" layoutY="20" onAction="#setVisiblePartName"/>
    <CheckBox fx:id="check_category" text="Category" layoutX="600" layoutY="20" onAction="#setVisibleCategory"/>
    <CheckBox fx:id="check_drawing" text="Drawing" layoutX="750" layoutY="20" onAction="#setVisibleDrawing"/>
    <CheckBox fx:id="check_rubber" text="Rubber" layoutX="900" layoutY="20" onAction="#setVisibleRubber"/>
    <CheckBox fx:id="check_diameter_at" text="Diameter AT" layoutX="1050" layoutY="20"
              onAction="#setVisibleDiameterAT"/>
    <CheckBox fx:id="check_diameter_at_tol" text="Diameter AT TOL" layoutX="1200" layoutY="20"
              onAction="#setVisibleDiameterATTOL"/>
    <CheckBox fx:id="check_length_l_at" text="Length L AT" layoutX="1350" layoutY="20" onAction="#setVisibleLengthLAT"/>
    <CheckBox fx:id="check_length_l_at_tol" text="Length L AT TOL" layoutX="1500" layoutY="20"
              onAction="#setVisibleLengthLATTOL"/>
    <CheckBox fx:id="check_diameter_it" text="Diameter IT" layoutX="1650" layoutY="20"
              onAction="#setVisibleDiameterIT"/>

    <CheckBox fx:id="check_diameter_it_tol" text="Diameter IT TOL" layoutX="150" layoutY="40"
              onAction="#setVisibleDiameterITTOL"/>
    <CheckBox fx:id="check_length_l_it" text="Length L IT" layoutX="300" layoutY="40" onAction="#setVisibleLengthLIT"/>
    <CheckBox fx:id="check_length_l_it_tol" text="Length L IT TOL" layoutX="450" layoutY="40"
              onAction="#setVisibleLengthLITTOL"/>
    <CheckBox fx:id="check_diameter_zt" text="Diameter ZT" layoutX="600" layoutY="40" onAction="#setVisibleDiameterZT"/>
    <CheckBox fx:id="check_diameter_zt_tol" text="Diameter ZT TOL" layoutX="750" layoutY="40"
              onAction="#setVisibleDiameterZTTOL"/>
    <CheckBox fx:id="check_length_l_zt" text="Length L ZT" layoutX="900" layoutY="40" onAction="#setVisibleLengthLZT"/>
    <CheckBox fx:id="check_length_l_zt_tol" text="Length L ZT TOL" layoutX="1050" layoutY="40"
              onAction="#setVisibleLengthLZTTOL"/>
    <CheckBox fx:id="check_cr_steg" text="Cr steg" layoutX="1200" layoutY="40" onAction="#setVisibleCrSteg"/>
    <CheckBox fx:id="check_cr_niere" text="Cr niere" layoutX="1350" layoutY="40" onAction="#setVisibleCrNiere"/>
    <CheckBox fx:id="check_ca" text="Ca" layoutX="1500" layoutY="40" onAction="#setVisibleCa"/>
    <CheckBox fx:id="check_ct" text="Ct" layoutX="1650" layoutY="40" onAction="#setVisibleCt"/>
    <CheckBox fx:id="check_ck" text="Ck" layoutX="1800" layoutY="40" onAction="#setVisibleCk"/>

    <TableView fx:id="table_parts" layoutX="50.0" layoutY="100.0" prefHeight="830.0"
               prefWidth="1810.0">
        <columns>
            <TableColumn fx:id="col_check_box" editable="false" prefWidth="75.0" sortable="false" text="Submit"/>
            <TableColumn fx:id="col_button_add_cat" editable="false" prefWidth="75.0" sortable="false"
                         text="Add Category"/>
            <TableColumn fx:id="col_part_number" editable="false" prefWidth="75.0" sortable="false" text="Part_number"/>
            <TableColumn fx:id="col_customer" editable="false" prefWidth="75.0" sortable="false" text="Customer_id"/>
            <TableColumn fx:id="col_part_name" editable="false" prefWidth="75.0" sortable="false" text="Part_name_id"/>
            <TableColumn fx:id="col_category" editable="false" prefWidth="75.0" sortable="false" text="Category_id"/>
            <TableColumn fx:id="col_drawing" editable="false" prefWidth="75.0" sortable="false" text="Drawing_id"/>
            <TableColumn fx:id="col_rubber" editable="false" prefWidth="75.0" sortable="false" text="Rubber"/>

            <TableColumn fx:id="col_diameter_at" editable="false" prefWidth="75.0" sortable="false" text="Diameter AT"/>
            <TableColumn fx:id="col_diameter_at_tol" editable="false" prefWidth="75.0" sortable="false"
                         text="Diameter AT TOL"/>
            <TableColumn fx:id="col_length_l_at" editable="false" prefWidth="75.0" sortable="false" text="Length L AT"/>
            <TableColumn fx:id="col_length_l_at_tol" editable="false" prefWidth="75.0" sortable="false"
                         text="Length L AT TOL"/>

            <TableColumn fx:id="col_diameter_it" editable="false" prefWidth="75.0" sortable="false" text="Diameter IT"/>
            <TableColumn fx:id="col_diameter_it_tol" editable="false" prefWidth="75.0" sortable="false"
                         text="Diameter IT TOL"/>
            <TableColumn fx:id="col_length_l_it" editable="false" prefWidth="75.0" sortable="false" text="Length L IT"/>
            <TableColumn fx:id="col_length_l_it_tol" editable="false" prefWidth="75.0" sortable="false"
                         text="Length L IT TOL"/>

            <TableColumn fx:id="col_diameter_zt" editable="false" prefWidth="75.0" sortable="false" text="Diameter ZT"/>
            <TableColumn fx:id="col_diameter_zt_tol" editable="false" prefWidth="75.0" sortable="false"
                         text="Diameter ZT TOL"/>
            <TableColumn fx:id="col_length_l_zt" editable="false" prefWidth="75.0" sortable="false" text="Length L ZT"/>
            <TableColumn fx:id="col_length_l_zt_tol" editable="false" prefWidth="75.0" sortable="false"
                         text="Length L ZT TOL"/>

            <TableColumn fx:id="col_cr_steg" editable="false" prefWidth="75.0" sortable="false" text="Cr steg"/>
            <TableColumn fx:id="col_cr_niere" editable="false" prefWidth="75.0" sortable="false" text="Cr niere"/>
            <TableColumn fx:id="col_ca" editable="false" prefWidth="75.0" sortable="false" text="Ca"/>
            <TableColumn fx:id="col_ct" editable="false" prefWidth="75.0" sortable="false" text="Ct"/>
            <TableColumn fx:id="col_ck" editable="false" prefWidth="75.0" sortable="false" text="Ck"/>
        </columns>
    </TableView>

    <Button layoutX="50.0" layoutY="10.0" mnemonicParsing="false" onAction="#onClickBackButton" text="Back"/>
    <Button layoutX="50.0" layoutY="60.0" mnemonicParsing="false" text="Confirm"
            onAction="#onClickConfirmButton"/>

    <Button layoutX="1820.0" layoutY="20.0" mnemonicParsing="false" onAction="#onClickFilterByCategory" text="Filter by Category"/>
    <Button layoutX="1820.0" layoutY="40.0" mnemonicParsing="false" onAction="#onClickFilterByPriority" text="Filter by Priority"/>
    <Button layoutX="1820.0" layoutY="60.0" mnemonicParsing="false" onAction="#onClickFilterByRating" text="Filter by Rating"/>
</AnchorPane>
