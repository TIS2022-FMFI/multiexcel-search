<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.161"
            fx:controller="frontend.Controllers.SearchControllers.SecondSearchControllers.SecondSearchController">

    <CheckBox fx:id="check_part_number" text="Part number" layoutX="100" layoutY="10" onAction="#setVisiblePartNumber"
              selected="true"/>
    <CheckBox fx:id="check_customer" text="Customer" layoutX="250" layoutY="10" onAction="#setVisibleCustomer"
              selected="true"/>
    <CheckBox fx:id="check_part_name" text="Part name" layoutX="400" layoutY="10" onAction="#setVisiblePartName"
              selected="true"/>
    <CheckBox fx:id="check_category" text="Category" layoutX="550" layoutY="10" onAction="#setVisibleCategory"
              selected="true"/>
    <CheckBox fx:id="check_drawing" text="Drawing" layoutX="700" layoutY="10" onAction="#setVisibleDrawing"
              selected="true"/>
    <CheckBox fx:id="check_rubber" text="Rubber" layoutX="850" layoutY="10" onAction="#setVisibleRubber"
              selected="true"/>
    <CheckBox fx:id="check_diameter_at" text="Diameter AT" layoutX="1000" layoutY="10" onAction="#setVisibleDiameterAT"
              selected="true"/>
    <CheckBox fx:id="check_diameter_at_tol" text="Diameter AT TOL" layoutX="1150" layoutY="10"
              onAction="#setVisibleDiameterATTOL" selected="true"/>
    <CheckBox fx:id="check_length_l_at" text="Length L AT" layoutX="1300" layoutY="10" onAction="#setVisibleLengthLAT"
              selected="true"/>
    <CheckBox fx:id="check_length_l_at_tol" text="Length L AT TOL" layoutX="1450" layoutY="10"
              onAction="#setVisibleLengthLATTOL" selected="true"/>
    <CheckBox fx:id="check_diameter_it" text="Diameter IT" layoutX="1600" layoutY="10" onAction="#setVisibleDiameterIT"
              selected="true"/>

    <CheckBox fx:id="check_diameter_it_tol" text="Diameter IT TOL" layoutX="100" layoutY="40"
              onAction="#setVisibleDiameterITTOL" selected="true"/>
    <CheckBox fx:id="check_length_l_it" text="Length L IT" layoutX="250" layoutY="40" onAction="#setVisibleLengthLIT"
              selected="true"/>
    <CheckBox fx:id="check_length_l_it_tol" text="Length L IT TOL" layoutX="400" layoutY="40"
              onAction="#setVisibleLengthLITTOL" selected="true"/>
    <CheckBox fx:id="check_diameter_zt" text="Diameter ZT" layoutX="550" layoutY="40" onAction="#setVisibleDiameterZT"
              selected="true"/>
    <CheckBox fx:id="check_diameter_zt_tol" text="Diameter ZT TOL" layoutX="700" layoutY="40"
              onAction="#setVisibleDiameterZTTOL" selected="true"/>
    <CheckBox fx:id="check_length_l_zt" text="Length L ZT" layoutX="850" layoutY="40" onAction="#setVisibleLengthLZT"
              selected="true"/>
    <CheckBox fx:id="check_length_l_zt_tol" text="Length L ZT TOL" layoutX="1000" layoutY="40"
              onAction="#setVisibleLengthLZTTOL" selected="true"/>
    <CheckBox fx:id="check_cr_steg" text="Cr steg" layoutX="1150" layoutY="40" onAction="#setVisibleCrSteg"
              selected="true"/>
    <CheckBox fx:id="check_cr_niere" text="Cr niere" layoutX="1300" layoutY="40" onAction="#setVisibleCrNiere"
              selected="true"/>
    <CheckBox fx:id="check_ca" text="Ca" layoutX="1450" layoutY="40" onAction="#setVisibleCa" selected="true"/>
    <CheckBox fx:id="check_ct" text="Ct" layoutX="1600" layoutY="40" onAction="#setVisibleCt" selected="true"/>
    <CheckBox fx:id="check_ck" text="Ck" layoutX="100" layoutY="70" onAction="#setVisibleCk" selected="true"/>

    <TableView fx:id="table_parts" layoutX="50.0" layoutY="110.0" prefHeight="830.0"
               prefWidth="1810.0">
        <columns>
            <TableColumn fx:id="col_check_box" editable="false" prefWidth="50.0" sortable="false" text="Submit"/>
            <TableColumn fx:id="col_button_add_cat" editable="false" prefWidth="70.0" sortable="false"
                         text="Add Category"/>
            <TableColumn fx:id="col_part_number" editable="false" prefWidth="75.0" sortable="false" text="Part number"/>
            <TableColumn fx:id="col_customer" editable="false" prefWidth="75.0" sortable="false" text="Customer"/>
            <TableColumn fx:id="col_part_name" editable="false" prefWidth="75.0" sortable="false" text="Part name"/>
            <TableColumn fx:id="col_category" editable="false" prefWidth="75.0" sortable="false" text="Category"/>
            <TableColumn fx:id="col_drawing" editable="false" prefWidth="200.0" sortable="false" text="Drawing"/>
            <TableColumn fx:id="col_rubber" editable="false" prefWidth="75.0" sortable="false" text="Rubber"/>

            <TableColumn fx:id="col_diameter_at" editable="false" prefWidth="75.0" sortable="false" text="Diameter AT"/>
            <TableColumn fx:id="col_diameter_at_tol" editable="false" prefWidth="75.0" sortable="false"
                         text="Diameter AT TOL"/>
            <TableColumn fx:id="col_length_l_at" editable="false" prefWidth="75.0" sortable="false" text="Length L AT"/>
            <TableColumn fx:id="col_length_l_at_tol" editable="false" prefWidth="75.0" sortable="false"
                         text="Length L AT TOL"/>

            <TableColumn fx:id="col_diameter_it" editable="false" prefWidth="75.0" sortable="false" text="Diameter IT"/>
            <TableColumn fx:id="col_diameter_it_tol" editable="false" prefWidth="75.0" sortable="false"
                         text="Diameter IT TOL"/>
            <TableColumn fx:id="col_length_l_it" editable="false" prefWidth="75.0" sortable="false" text="Length L IT"/>
            <TableColumn fx:id="col_length_l_it_tol" editable="false" prefWidth="75.0" sortable="false"
                         text="Length L IT TOL"/>

            <TableColumn fx:id="col_diameter_zt" editable="false" prefWidth="75.0" sortable="false" text="Diameter ZT"/>
            <TableColumn fx:id="col_diameter_zt_tol" editable="false" prefWidth="75.0" sortable="false"
                         text="Diameter ZT TOL"/>
            <TableColumn fx:id="col_length_l_zt" editable="false" prefWidth="75.0" sortable="false" text="Length L ZT"/>
            <TableColumn fx:id="col_length_l_zt_tol" editable="false" prefWidth="75.0" sortable="false"
                         text="Length L ZT TOL"/>

            <TableColumn fx:id="col_cr_steg" editable="false" prefWidth="75.0" sortable="false" text="Cr steg"/>
            <TableColumn fx:id="col_cr_niere" editable="false" prefWidth="75.0" sortable="false" text="Cr niere"/>
            <TableColumn fx:id="col_ca" editable="false" prefWidth="75.0" sortable="false" text="Ca"/>
            <TableColumn fx:id="col_ct" editable="false" prefWidth="75.0" sortable="false" text="Ct"/>
            <TableColumn fx:id="col_ck" editable="false" prefWidth="75.0" sortable="false" text="Ck"/>
        </columns>
    </TableView>

    <Button fx:id="backButton" layoutX="20.0" layoutY="15.0" mnemonicParsing="false" onAction="#onClickBackButton"/>
    <Button fx:id="confirmButton" layoutX="1750.0" layoutY="15.0" mnemonicParsing="false"
            onAction="#onClickConfirmButton"/>

    <Button layoutX="250.0" layoutY="70.0" mnemonicParsing="false" onAction="#onClickFilterByCategory"
            text="Filter by Category" fx:id="categoryFilter"/>
    <Button fx:id="filterByPriorityButton" layoutX="400.0" layoutY="70.0" mnemonicParsing="false"
            onAction="#onClickFilterByPriority" text="Filter by Priority"/>
    <Button fx:id="filterByRatingButton" layoutX="550.0" layoutY="70.0" mnemonicParsing="false"
            onAction="#onClickFilterByRating" text="Filter by Rating"/>
</AnchorPane>
