            <TableColumn fx:id="col_diameter_it" editable="false" prefWidth="101.0" sortable="false" style="-fx-alignment: CENTER;" text="Diameter IT" />
            <TableColumn fx:id="col_length_l_it" editable="false" prefWidth="101.0" sortable="false" style="-fx-alignment: CENTER;" text="Length L IT" />
            <TableColumn fx:id="col_diameter_zt" editable="false" prefWidth="101.0" sortable="false" style="-fx-alignment: CENTER;" text="Diameter ZT" />
