    <Button fx:id="button_category_filter" layoutX="438.0" layoutY="80.0" mnemonicParsing="false" onAction="#onClickCategoryFilterButton" text="Filter by category" />
    <Button fx:id="button_delte_selected" layoutX="1714.0" layoutY="524.0" mnemonicParsing="false" onAction="#deleteSelectedQueries" text="Delete selected" />
</AnchorPane>
