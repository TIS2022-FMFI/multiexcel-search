            <TableColumn fx:id="col_cr_niere" editable="false" prefWidth="101.0" sortable="false" style="-fx-alignment: CENTER;" text="Cr niere" />
            <TableColumn fx:id="col_ca" editable="false" prefWidth="101.0" sortable="false" style="-fx-alignment: CENTER;" text="Ca" />
            <TableColumn fx:id="col_ct" editable="false" prefWidth="101" sortable="false" style="-fx-alignment: CENTER;" text="Ct" />
            <TableColumn fx:id="col_ck" editable="false" prefWidth="101" sortable="false" style="-fx-alignment: CENTER;" text="Ck" />
            <TableColumn fx:id="col_date" editable="false" prefWidth="101" sortable="false" style="-fx-alignment: CENTER;" text="Date" />
        </columns>
    </TableView>
    <Button fx:id="button_previous_page" layoutX="123.0" layoutY="900.0" mnemonicParsing="false" onAction="#onPreviousPageButtonClick" text="previous page" />
    <Button fx:id="button_next_page" layoutX="1541.0" layoutY="900.0" mnemonicParsing="false" onAction="#onNextPageButtonClick" text="next page" />
    <Label fx:id="label_page_index" contentDisplay="CENTER" layoutX="847.0" layoutY="900.0" text="Page 0 / 100" textAlignment="CENTER" textOverrun="CLIP" />
    <DatePicker fx:id="date_picker_to" layoutX="1097.0" layoutY="84.0" promptText="to" />
    <DatePicker fx:id="date_picker_from" layoutX="755.0" layoutY="81.0" promptText="from" />
    <Button fx:id="button_date_from_clear" layoutX="936.0" layoutY="83.0" mnemonicParsing="false" onAction="#onClearDateFromClick" text="Clear" />
    <Button fx:id="button_date_to_clear" layoutX="1277.0" layoutY="84.0" mnemonicParsing="false" onAction="#onClearDateToClick" text="Clear" />
