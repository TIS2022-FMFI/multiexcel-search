<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.161"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="frontend.Controllers.CategoryManagementControllers.CategoryMainController">

    <ListView fx:id="categoryList" layoutX="50.0" layoutY="110.0" prefHeight="800.0" prefWidth="800.0"/>
    <Button fx:id="addButton" layoutX="50.0" layoutY="25.0" mnemonicParsing="false" onAction="#addCategory"
            prefHeight="44.0" prefWidth="120.0"/>
    <ListView fx:id="partList" layoutX="1000.0" layoutY="107.0" prefHeight="700.0" prefWidth="870.0"/>

    <Button fx:id="prevPageButton" layoutX="1050.0" layoutY="850.0" mnemonicParsing="false" onAction="#prevPage"
            prefHeight="55.0" prefWidth="55.0" text="&lt;"/>
    <Button fx:id="nextPageButton" layoutX="1750.0" layoutY="850.0" mnemonicParsing="false" onAction="#nextPage"
            prefHeight="55.0" prefWidth="55.0" text="&gt;"/>
    <Label alignment="CENTER" layoutX="1352.0" layoutY="57.0" prefHeight="45.0" prefWidth="166.0"
           textAlignment="CENTER">Popularity list:
    </Label>
    <Label fx:id="pageLabel" alignment="CENTER" layoutX="1352.0" layoutY="855.0" prefHeight="45.0" prefWidth="166.0"/>
    <Label fx:id="noPartsLabel" alignment="CENTER" layoutX="1220.0" layoutY="430.0" prefHeight="54.0" prefWidth="465.0"
           textAlignment="CENTER" visible="false"/>

</AnchorPane>
