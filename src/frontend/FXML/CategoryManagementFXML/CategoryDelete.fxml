<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="186.0" prefWidth="573.0"
            xmlns="http://javafx.com/javafx/8.0.161"
            fx:controller="frontend.Controllers.CategoryManagementControllers.CategoryDeleteController">
    <Text fx:id="deleteTextArea" layoutX="38.0" layoutY="56.0"
          text="Deleting a category will clear your search history. Do you wish to continue?"/>
    <Button fx:id="deleteButton" layoutX="403.0" layoutY="127.0" mnemonicParsing="false" onAction="#deleteCategory"
            prefHeight="30.0" prefWidth="130.0" text="Delete category"/>
    <Button fx:id="closeButton" layoutX="38.0" layoutY="127.0" mnemonicParsing="false" onAction="#closeDeleteWindow"
            prefHeight="30.0" prefWidth="130.0" text="Back"/>
    <Label fx:id="errorLabel" layoutX="38.0" layoutY="78.0" prefHeight="30.0" prefWidth="474.0" textFill="#ff0000"/>
</AnchorPane>
