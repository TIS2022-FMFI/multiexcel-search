<AnchorPane prefHeight="300.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="frontend.Controllers.CategoryManagementControllers.CategoryEditController">

    <Text fx:id="editTextArea" layoutX="86.0" layoutY="157.0" text="New category name:" />
    <TextField fx:id="categoryNewNameField" layoutX="265.0" layoutY="137.0" prefHeight="30.0" prefWidth="300.0" />
    <Button fx:id="editButton" layoutX="710.0" layoutY="30.0" mnemonicParsing="false" onAction="#renameCategory" prefHeight="80.0" prefWidth="150.0" text="Rename category" />
    <Button fx:id="closeButton" layoutX="710.0" layoutY="162.0" mnemonicParsing="false" onAction="#closeEditWindow" prefHeight="80.0" prefWidth="150.0" text="Back" />
    <Label fx:id="errorLabel" layoutX="140.0" layoutY="207.0" prefHeight="30.0" prefWidth="500.0" textFill="#ff0000" />

</AnchorPane>
